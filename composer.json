{
    "name": "binarytorch/blogged",
    "description": "Write gorgeous articles for your Laravel applications using MarkDown",
    "keywords": ["laravel", "markdown", "blog", "articles"],
    "license": "MIT",
    "type": "library",
    "support": {
        "issues": "https://github.com/saleem-hadad/blogged/issues",
        "source": "https://github.com/saleem-hadad/blogged"
    },
    "authors": [
        {
            "name": "Saleem Hadad",
            "email": "saleem@binary-torch.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "symfony/dom-crawler": "^5.0",
        "mockery/mockery": "^1.2",
        "mews/purifier": "^3.1",
        "michelf/php-markdown": "^1.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "laravel/framework": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "orchestra/database": "~3.4.0|~3.5.0|~3.6.0|~3.7.0|~3.8.0|^4.0",
        "orchestra/testbench": "~3.4.0|~3.5.0|~3.6.0|~3.7.0|~3.8.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "BinaryTorch\\Blogged\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BinaryTorch\\Blogged\\Tests\\": "tests/"
      }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "BinaryTorch\\Blogged\\BloggedServiceProvider"
            ]
        }
    }
}
